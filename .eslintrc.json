{
  "env": {
    "jest": true,
    "browser": true,
    "es2021": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module",
    "warnOnUnsupportedTypeScriptVersion": false
  },
  "plugins": ["react", "jsx-a11y", "import", "@typescript-eslint", "react-hooks"],
  "rules": {
    "react/prop-types": 0,
    "@typescript-eslint/no-var-requires": 0,
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [
      0,
      {
        "extensions": [".js", ".jsx"]
      }
    ], //should add ".ts" if typescript project
    "no-template-curly-in-string": 2,
    "no-loss-of-precision": 2,
    "no-irregular-whitespace": 2,
    "constructor-super": 0,
    "getter-return": 2,
    "no-class-assign": 2,
    "no-cond-assign": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-dupe-class-members": 2,
    "no-ex-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-misleading-character-class": 2,
    "no-new-symbol": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-prototype-builtins": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable-loop": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unused-labels": 2,
    "require-atomic-updates": 2,
    "require-yield": 2,
    "strict": "warn",
    "use-isnan": 2,
    "no-useless-concat": 2,
    "eqeqeq": [
      "error",
      "always",
      {
        "null": "ignore"
      }
    ],
    // The below are some, but not all, of the rules from eslint-plugin-react:recommended https://github.com/yannickcr/eslint-plugin-react#recommended (all errors set to warn)
    "react/display-name": 2,
    "react/no-array-index-key": 1,
    "react/require-default-props": 1,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-undef": 2,
    "react/no-children-prop": 2,
    "react/no-danger-with-children": 2,
    "react/no-deprecated": 2,
    "react/no-direct-mutation-state": 2,
    "react/no-find-dom-node": 2,
    "react/no-is-mounted": 2,
    "jsx-a11y/anchor-is-valid": 1,
    "react/no-render-return-value": 2,
    "react/no-string-refs": 2,
    "react/no-unescaped-entities": 2,
    "react/no-unknown-property": 2,
    "react/require-render-return": 2,
    // Some additional React rules
    "react/no-danger": 0,
    "react/no-did-mount-set-state": 2,
    "react/no-did-update-set-state": 2,
    //here done
    "react/jsx-uses-vars": "error",
    "react/jsx-uses-react": "error",
    "no-self-compare": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-self-assign": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-dupe-args": 2,
    "no-dupe-else-if": 2,
    "no-debugger": "warn",
    // "max-lines": [
    //     "error",
    //     {
    //         "max": 700
    //     }
    // ],
    "no-fallthrough": 2,
    "no-array-constructor": 2,
    "no-extra-boolean-cast": 2,
    "no-func-assign": 2,
    "no-import-assign": 2,
    "no-global-assign": "error",
    "no-eval": 2,
    "array-callback-return": "error",
    "no-implicit-globals": "error",
    "no-useless-catch": 2,
    "no-with": 2,
    "valid-typeof": 2,
    "default-param-last": 2,
    "import/newline-after-import": [
      "error",
      {
        "count": 1
      }
    ],
    "space-before-function-paren": [0, "always"],
    "no-trailing-spaces": "error",
    "arrow-spacing": "error",
    "no-multiple-empty-lines": [
      2,
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "react/button-has-type": "error",
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define": "warn",
    "@typescript-eslint/no-empty-function": "off",
    "react/jsx-indent": [0],
    "prefer-const": 2,
    "react/prefer-es6-class": 0,
    "class-methods-use-this": [0],
    "no-unsafe-optional-chaining": 0,
    "@typescript-eslint/ban-types": 0,
    "no-undef": [1],
    "no-case-declarations": [1],
    "no-return-assign": [1],
    "no-param-reassign": [2],
    "camelcase": [0],
    "no-underscore-dangle": [0, "always"],
    "no-var": [2],
    "@typescript-eslint/no-unused-vars": [2],
    "react/jsx-key": 2,
    "no-useless-escape": 1,
    "no-duplicate-imports": 2,
    "prefer-destructuring": 2,
    "no-loop-func": 2,
    "for-direction": 2,
    "no-async-promise-executor": 0,
    "no-await-in-loop": 2,
    "no-compare-neg-zero": 2,
    "init-declarations": 0,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-restricted-globals": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-undefined": 0,
    "no-restricted-exports": 2,
    "no-restricted-imports": 2,
    "object-shorthand ": 0,
    "operator-assignment": 0,
    "padding-line-between-statements": 2,
    "prefer-exponentiation-operator": 2,
    "prefer-object-spread": 1,
    "quote-props": 0,
    "quotes": 0,
    "semi": 2, //semi
    "no-alert": 1,
    // require the use of === and !==
    "no-div-regex": 2,
    "block-scoped-var": 2,
    "dot-notation": [
      0,
      {
        "allowKeywords": true
      }
    ],
    "react/jsx-no-bind": [
      "error",
      {
        "ignoreDOMComponents": true,
        "ignoreRefs": true,
        "allowArrowFunctions": true,
        "allowFunctions": false,
        "allowBind": true
      }
    ],
    // require default case in switch statements
    "default-case": 2,
    "no-console": [
      "error",
      {
        "allow": [""]
      }
    ],
    "yield-star-spacing": ["error", "both"],
    "wrap-regex": 1,
    "unicode-bom": 2,
    "template-tag-spacing": ["error", "always"],
    "switch-colon-spacing": "error",
    "space-unary-ops": "error",
    "space-in-parens": "error",
    "space-before-blocks": ["error", "always"],
    "semi-style": ["error", "last"],
    "semi-spacing": "error",
    "object-curly-spacing": ["error", "always"],
    "no-tabs": "error",
    "no-multi-spaces": [
      "error",
      {
        "ignoreEOLComments": false
      }
    ],
    "no-mixed-spaces-and-tabs": ["error", "smart-tabs"],
    "key-spacing": [
      "error",
      {
        "afterColon": true
      }
    ],
    "generator-star-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "eol-last": 0,
    "comma-style": ["error", "last"],
    "comma-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "arrow-parens": ["error", "always"],
    "array-bracket-spacing": "error",
    "yoda": "error",
    "require-await": "error",
    "jsx-a11y/alt-text": [
      2,
      {
        "elements": ["img", "input[type=\"image\"]"],
        "img": ["Image"],
        "input[type=\"image\"]": ["InputImage"]
      }
    ]
  }
}
